<div style="display: flex">
  <div>
    <button
      class="under up-arrow"
      (click)="lastL(this.firstColumn)"
      value="1"
      #upButtonFirstColumn
    ></button>
    <button class="under" value="1" #middleButtonFirstColumn>
      {{ this.firstColumn.letters[firstColumn.currentIndex] }}
    </button>
    <button
      class="under down-arrow"
      value="2"
      (click)="nextL(this.firstColumn)"
      #downButtonFirstColumn
    ></button>
  </div>
  <div>
    <button
      class="under up-arrow"
      (click)="lastL(this.secondColumn)"
      value="1"
      #upButtonSecondColumn
    ></button>
